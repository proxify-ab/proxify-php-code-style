{
    "name": "proxify/proxify-php-code-style",
    "description": "A PHP code style package that helps enforce consistent code formatting and style conventions using Proxify standards.",
    "keywords": [
        "proxify",
        "proxify-php-code-style"
    ],
    "homepage": "https://github.com/proxify/proxify-php-code-style",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.0",
        "laravel/pint": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Proxify\\ProxifyPhpCodeStyle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Proxify\\ProxifyPhpCodeStyle\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/proxify-cs-checker",
        "bin/proxify-cs-fixer"
    ],
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}